# n8n Configuration
N8N_HOST=n8n
N8N_PORT=5678
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=user
N8N_BASIC_AUTH_PASSWORD=password
N8N_LOG_LEVEL=info

# Example API Credentials (used in credential templates)
# These will be substituted into credential files using {{$env.VARIABLE_NAME}}

# Example API Basic Auth
API_USERNAME=your_username
API_PASSWORD=your_password

# Example Slack API Token
SLACK_ACCESS_TOKEN=xoxb-your-slack-token

# Add your own environment variables here for credential substitution
# MY_SERVICE_API_KEY=your_key
# MY_SERVICE_SECRET=your_secret
# DATABASE_PASSWORD=your_db_password
# ...
